{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "data": {"url": "data/sp500.csv"},
    "params": [{
      "name": "brush",
      "select": {
        "type": "interval",
        "encodings": ["x"]
      },
      "views": ["overview"]
    }],
    "vconcat": [{
      "width": 480,
      "mark": "area",
      "encoding": {
        "x": {
          "field": "date",
          "type": "temporal",
          "scale": {"domain": {"selection": "brush"}},
          "axis": {"title": ""}
        },
        "y": {"field": "price", "type": "quantitative"}
      }
    }, {
      "name": "overview",
      "width": 480,
      "height": 60,
      "mark": "area",
      "encoding": {
        "x": {
          "field": "date",
          "type": "temporal"
        },
        "y": {
          "field": "price",
          "type": "quantitative",
          "axis": {"tickCount": 3, "grid": false}
        }
      }
    }]
  }
  